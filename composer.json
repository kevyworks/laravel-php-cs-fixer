{
    "name": "kevyworks/laravel-php-cs-fixer",
    "description": "A PHP-CS-Fixer bridge for use via Artisan CLI on Laravel 5.5+",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.5.0",
        "friendsofphp/php-cs-fixer": "~2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kevyworks",
            "email": "d3@kevyworks.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Kevyworks\\LaravelPhpCsFixer\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Kevyworks\\LaravelPhpCsFixer\\": "src"
        }
    }
}
